<!DOCTYPE html>
<head>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
</head>
<html>
  <body>
    <div class='container'>
    <table class='table'>
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Age</th>
          <th>Created At</th>
          <th>Updated At</th>
          <th>User ID</th>
        </tr>
      </thead>
      <tbody>
        <% for (var i = 0; i < all_users.length; i++) { %>
        <tr>
          <td> <%= all_users[i].first_name %> </td>
          <td> <%= all_users[i].last_name %></td>
          <td> <%= all_users[i].age %> </td>
          <td> <%= all_users[i].createdAt %> </td>
          <td> <%= all_users[i].updatedAt %> </td>
          <td> <%= all_users[i].id %> </td>
          <td><a href="/edit/<%= all_users[i].id %>">Edit User</a></td>
        </tr>
        <% } %>
      </tbody>
    </table>
    <form method='GET' action='/add'><button type='submit' class='btn btn-primary'>Add a New User</button></form>
    </div>
  </body>
</html>
